{
  "functions": {
    "api/proxy.js": {
      "runtime": "nodejs14.x",
      "memory": 512
    }
  },
  "routes": [
    {
      "src": "/api/vtt",
      "methods": ["GET"],
      "dest": "api/proxy.js"
    },
    {
      "src": "/api/m3u8",
      "methods": ["GET"],
      "dest": "api/proxy.js"
    },
    {
      "src": "/api/text",
      "methods": ["GET"],
      "dest": "api/proxy.js"
    },
    {
      "src": "/api/json",
      "methods": ["GET"],
      "dest": "api/proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
